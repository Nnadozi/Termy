{
  "expo": {
    "name": "Termy",
    "slug": "Termy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "termy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nnadozi.termy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF6A00"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nnadozi.termy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 1000,
          "resizeMode": "contain",
          "backgroundColor": "#FF6A00"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/DMSans-Regular.ttf",
            "./assets/fonts/DMSans-Bold.ttf",
            "./assets/fonts/DMSans-Italic.ttf"
          ]
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow Termy to access your Face ID biometric data."
        }
      ],
      ["expo-sqlite"],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3f69e2d6-5620-4b05-a7c5-c9771c732a84"
      }
    }
  }
}
